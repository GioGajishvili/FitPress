<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation2"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.finalproject.fragments.ProfileFragment"
        android:label="ProfileFragment" >
        <action
            android:id="@+id/action_profileFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>

    <fragment
        android:id="@+id/workoutsFragment"
        android:name="com.example.finalproject.fragments.WorkoutsFragment"
        android:label="WorkoutsFragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.finalproject.fragments.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_workoutsFragment"
            app:destination="@id/workoutsFragment" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.finalproject.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>